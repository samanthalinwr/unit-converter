  "predeploy": "GITHUB_PAGES=true next build",
  "deploy": "rm -rf ../docs && mkdir ../docs && cp -r out/* ../docs/ && touch ../docs/.nojekyll && cd .. && git add docs && git commit -m \"Deploy to Pages\" || echo \"no changes\" && git push",
  "ship": "npm run predeploy && npm run deploy"
  },
